/* pages/searchResult/searchResult.wxss */
.container {
  display: flex;
  flex-direction: column;
  align-items: center;
  box-sizing: border-box;
  padding-bottom:12rpx;
}
.container .circleList{
  margin-top:84rpx;
}
page{
  background:#fff;
}
.search{
  display: flex;
  justify-content: space-between;
  width: 92%;
  position: fixed;
  left: 4%;
  top: 0;
  background-color: white;
  z-index: 1;
}
.search-icon{
  margin-top: 10rpx;
  margin-left: 40rpx;
  width: 33rpx;
  height: 33rpx;
  display: inline-block;
  vertical-align: text-top;
}
.search-input{
  margin-left: 20rpx;
  font-size: 28rpx;
  color: #333;
    display: inline-block;
  margin-top: 4rpx;
}
.search-left{
  display: flex;
  background: #f7f7f7;
  border-radius: 4rpx;
  width: 84%;
  text-align: left;
  padding: 15rpx;
  font-size: 24rpx;
}
.cancel{
  font-size: 28rpx;
  color: #333;
  line-height: 80rpx;
}
.content{
padding-top: 100rpx;
}